.promo_section
  .container
    .intro-wp
      %h2 Take the Ideal Me Challenge: 12 Goals, 1 Per Month, Maximum Results in Minimum Time
      .cnt
        .video
          %iframe{:allowfullscreen => "", :frameborder => "0", :height => "225", :src => "//www.youtube.com/embed/piLWFiS1fN8", :width => "370"}
          %span.video-bg &nbsp;
        .text
          %ul
            %li Your Ideal Me Challenge:
            %li 1. Join the community
            %li 2. Choose your goals
            %li 3. Get hand-picked, user-reviewed course recommendations
        .join-box
          = form_for :user, url: user_session_path do |f|
            = link_to "Join with Facebook", user_omniauth_authorize_path(:facebook), :class => "fb-join-btn"
            .or-text
              %span.line &nbsp;
              %span.tx Or
            .field-wp
              %span.email-icon &nbsp;
              = f.email_field :email, :placeholder => "Email", :class => "field"
            .field-wp
              %span.pass-icon &nbsp;
              = f.text_field :password, :placeholder => "Password", :class => "field"
            .join-btn-wp
              %span.strike &nbsp;
              = f.submit "Join Now (It's Free)", :class => "join-btn"
        .clear &nbsp;
    .clear
