<table class="twelve">
  <thead>
  <tr>
    <th>User</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= link_to "#{user.firstname} #{user.lastname}", user_path(user) %></td>
        <td><%= user.email %></td>
      </tr>
  <% end %>
  </tbody>
</table>
